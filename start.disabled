#!/usr/bin/env bash
set -euo pipefail

# breadcrumb so we can confirm this file actually ran
echo "nbclassic start executed at $(date)" > /home/jovyan/.nbclassic_started

# Launch nbclassic on Jupyter Server, land on /tree
exec jupyter nbclassic \
  --ServerApp.default_url=/tree \
  --ip=0.0.0.0 --port="${PORT:-8888}" --no-browser
